<?xml version="1.0"?>
<package >
	<metadata>
		<id>{id}</id>
		<version>{version}</version>
		<title>Storm.Api</title>
		<authors>{author}</authors>
		<owners>Storm</owners>
		<license type="expression">MIT</license>
		<projectUrl>https://github.com/Julien-Mialon/Storm.Api</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Storm framework for API with aspnet core</description>
		<releaseNotes>{release_notes}</releaseNotes>
		<copyright>Copyright 2019</copyright>
		<tags>storm api aspnet aspnetcore cqrs</tags>
		<dependencies>
			<group targetFramework=".NETCoreApp3.1">
				<dependency id="Storm.Api.Core" version="{storm.api}" exclude="Build,Analyzers" />
				<dependency id="Storm.Api.Dtos" version="{storm.api}" exclude="Build,Analyzers" />
				<dependency id="Microsoft.AspNetCore.Mvc.NewtonsoftJson" version="3.1.1" exclude="Build,Analyzers" />
				<dependency id="Swashbuckle.AspNetCore" version="5.0.0" exclude="Build,Analyzers" />
				<dependency id="Swashbuckle.AspNetCore.Annotations" version="5.0.0" exclude="Build,Analyzers" />
			</group>
		</dependencies>
		<frameworkReferences>
			<group targetFramework=".NETCoreApp3.1">
				<frameworkReference name="Microsoft.AspNetCore.App" />
			</group>
		</frameworkReferences>
	</metadata>
</package>